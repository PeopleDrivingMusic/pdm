# Database Configuration
DATABASE_URL="postgresql://{username}:{password}@{host}:{port}/{database}"

# Alternative Database Configuration (for manual setup)
DB_HOST={host}
DB_PORT={port}
DB_USERNAME={username}
DB_PASSWORD={password}
DB_NAME={database}

# Environment
NODE_ENV=development

